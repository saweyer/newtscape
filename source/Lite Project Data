// for Newt's Cape lite (navigation controls)// for non-standalone bookspartData := {hrefScriptG:	kGoToHREFclickShare,	// !!!phrefScript: 	func(obj,unit)			// !!!p. for old books	call kGoToHREFclickShare with (obj, unit, aeTap),bookOpenScript: func(obj)	call kBookOpenScriptShare with (		obj, kDummyButton, kHistoryButton, kBackButton, kForwardButton)};InstallScript := func(partFrame, removeFrame)begin	local gData := call kInitGlobal with (nil, nil);	//removeFrame.removeSlots := [];	local sym, val;	foreach sym,val in partFrame.partData	do begin		sym := EnsureInternal(sym);		gData.(sym) := val;		removeFrame.(sym) := true;		end;end;RemoveScript := func(removeFrame)begin	local gData := call kInitGlobal with (nil, nil);	local sym, val;	foreach sym,val in removeFrame	do RemoveSlot(gData, sym);end;