OpenResFile(Home & "Resources");myIcons := MakeExtrasIcons([	{ unhilitedRsrcSpec:"part icon4", hilitedRsrcSpec:"part icon4hi", bitDepth:4 },	{ unhilitedRsrcSpec:"part icon", hilitedRsrcSpec:nil, bitDepth:1 }], "part icon!", nil);CloseResFile();foreach slot, value in myIcons do	SetPartFrameSlot(slot, value);SetPartFrameSlot('text, kEncodingTitle & " Encoding");InstallScript := func(partFrame, removeFrame)begincall kRegNTEncodingFunc with (kEncodingSymbol, GetLayout("NTEncodingDef"))end;RemoveScript := func(removeFrame)begincall kUnRegNTEncodingFunc with (kEncodingSymbol)end;